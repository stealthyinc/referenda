    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.0.0",
    "react-router-modal": "^1.5.2",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "seamless-immutable": "^7.1.4",
    "victory-native": "^32.0.2"
